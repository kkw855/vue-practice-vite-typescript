{
  "name": "vue-practice-router-typescript",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{vue,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,json,css}": "prettier --write"
  },
  "scripts": {
    "check_conflicts_prettier": "eslint-config-prettier .eslintrc.json",
    "check_format": "prettier src/**/*.{css,vue,ts} --check",
    "format": "prettier --write src/**/*.{css,vue,ts}",
    "lint": "eslint src/**/*.{vue,ts}",
    "test": "vitest --passWithNoTests",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.5"
  }
}
